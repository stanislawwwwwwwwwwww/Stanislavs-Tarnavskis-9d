
-- create
CREATE TABLE EMPLOYEE (
  Id INTEGER NOT NULL,
  Customerid INTEGER NOT NULL,
  Name TEXT NOT NULL,
  Surname TEXT NOT NULL,
  Customer_name TEXT NOT NULL
);

